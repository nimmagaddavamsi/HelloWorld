import org.json.JSONObject;

public class DynamicObjectBuilder {

    public static void main(String[] args) {
        // Sample JSON string with key-value pairs
        String jsonString = "{\"name\": \"John\", \"age\": 30, \"city\": \"New York\"}";

        // Parse the JSON string into a JSONObject
        JSONObject jsonObject = new JSONObject(jsonString);

        // Build a dynamic object from the JSON data
        DynamicObject dynamicObject = buildDynamicObject(jsonObject);

        // Print the dynamic object
        System.out.println(dynamicObject);
    }

    private static DynamicObject buildDynamicObject(JSONObject jsonObject) {
        DynamicObject dynamicObject = new DynamicObject();
        for (String key : jsonObject.keySet()) {
            dynamicObject.addProperty(key, jsonObject.get(key));
        }
        return dynamicObject;
    }
}

class DynamicObject {
    // You can use a Map or any other data structure to hold key-value pairs
    private final JSONObject properties;

    public DynamicObject() {
        properties = new JSONObject();
    }

    public void addProperty(String key, Object value) {
        properties.put(key, value);
    }

    @Override
    public String toString() {
        return properties.toString();
    }
}
