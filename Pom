import org.json.JSONException;
import org.json.JSONObject;

public class JsonObjectComparator {

    public static void main(String[] args) throws JSONException {
        JSONObject json1 = new JSONObject("{\"name\":\"John\",\"age\":30,\"city\":\"New York\"}");
        JSONObject json2 = new JSONObject("{\"age\":35,\"city\":\"Los Angeles\",\"country\":\"USA\"}");

        JSONObject mergedJson = mergeJsonObjects(json1, json2);

        System.out.println("Merged JSON:");
        System.out.println(mergedJson.toString(4));
    }

    private static JSONObject mergeJsonObjects(JSONObject obj1, JSONObject obj2) throws JSONException {
        JSONObject mergedJson = new JSONObject();

        // Merge keys from obj1
        for (String key : obj1.keySet()) {
            mergedJson.put(key, obj1.get(key));
        }

        // Merge keys from obj2, overwrite existing values if necessary
        for (String key : obj2.keySet()) {
            if (mergedJson.has(key)) {
                // Handle conflict: overwrite value from obj1 with value from obj2
                // You can customize this logic based on your requirements
                mergedJson.put(key, obj2.get(key));
            } else {
                // Key does not exist in obj1, add it to mergedJson
                mergedJson.put(key, obj2.get(key));
            }
        }

        return mergedJson;
    }
}
